from analyze import models
from django.utils import timezone

def main():
    now = timezone.now()
    char_banner = models.CharacterBanner(name="basecharbanner", enddate=now)
    char_banner.save()
    char_banner.rateups.add(models.Character.objects.get(id=1))
    # char_banner.save()
    weapon_banner = models.WeaponBanner(name="baseweaponbanner",enddate=now)
    weapon_banner.save()
    weapon_banner.rateups.add(models.Weapon.objects.get(id=1))
    # weapon_banner.save()

if __name__ == '__main__':
    main()

from analyze import models
from django.utils import timezone
now = timezone.now()
char_banner = models.CharacterBanner(name="basecharbanner", enddate=now)
char_banner.save()